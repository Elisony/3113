
.tab-nav{
	width: 100%;
	height: 45px;
	background-color: #ccc;
	position: fixed;
	bottom: 0px;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.tab-left{
	width: 80%;
	height: 45px;
	background-color: #308bd1;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
}
.tab-right{
	width: 20%;
	height: 45px;
	background-color: #fff;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.left-refresh{
	width: 21px;
	height: 17px;
}
.left-refresh>image{
	width: 100%;
	height: 100%;
}
.refresh-title{
	font-size: 15px;
	color: #fff;
	padding-left: 10px;
}
.right-task{
	width: 21px;
	height: 21px;
}
.right-task>image{
	width: 100%;
	height: 100%;
}
.right-title{
	font-size: 10px;
	color: #333333;
}

